</space>
<space name="coil_005" node="coil_space_005">
  <transformation rotation_euler="0. 0. 0."/> 

  <transformation displacement="{0.} {0.} {-0.200000}"/>
</space>
</space>

<cylinder_space name="world_space" z1="-50" z2="50" r="20"/>
<space name="world" node="world_space">
  <space name="project8" tree="project8_assembly">
    <transformation rotation_axis_angle="[alignment] 90. 90."/>
  </space>
</space>

<!-- electromagnets -->
<electromagnet name="electromagnet_solenoid" spaces="world/project8/@coil_tag_000" current="{-1*250.000000*[current] }"/>
<electromagnet name="electromagnet_solenoid" spaces="world/project8/@coil_tag_001" current="{-1*250.000000*[current] }"/>
<electromagnet name="electromagnet_solenoid" spaces="world/project8/@coil_tag_002" current="{-1*62.413686*[current] }"/>
<electromagnet name="electromagnet_solenoid" spaces="world/project8/@coil_tag_003" current="{-1*62.413686*[current] }"/>
<electromagnet name="electromagnet_solenoid" spaces="world/project8/@coil_tag_004" current="{-1*-125.000000*[current] }"/>
<electromagnet name="electromagnet_solenoid" spaces="world/project8/@coil_tag_005" current="{-1*-125.000000*[current] }"/>

<!-- VTK appearance -->
<appearance name="app_magnet" color="0 255 96 96" arc="72" surfaces="world/project8/coil_000/#"/>
<appearance name="app_magnet" color="0 255 96 96" arc="72" surfaces="world/project8/coil_001/#"/>
<appearance name="app_magnet" color="0 255 96 96" arc="72" surfaces="world/project8/coil_002/#"/>
<appearance name="app_magnet" color="0 255 96 96" arc="72" surfaces="world/project8/coil_003/#"/>
<appearance name="app_magnet" color="0 255 96 96" arc="72" surfaces="world/project8/coil_004/#"/>
<appearance name="app_magnet" color="0 255 96 96" arc="72" surfaces="world/project8/coil_005/#"/>

</geometry>

<kemfield>

    <!-- fields -->


    <electromagnet_field
        name="trap_field"
        file="Project8Magnets.kbd"
        system="world/project8"
        spaces="world/project8/@magnet_tag"
    >
        <zonal_harmonic_field_solver
            number_of_bifurcations="-1"
            convergence_ratio=".99"
            convergence_parameter="1.e-15"
            proximity_to_sourcepoint="1.e-12"
            number_of_central_coefficients="500"
            use_fractional_central_sourcepoint_spacing="true"
            central_sourcepoint_fractional_distance="1e-2"
            central_sourcepoint_spacing="2.e-5"
            number_of_remote_coefficients="200"
            remote_sourcepoint_start="-1.e-1"
            remote_sourcepoint_end="1.e-1"
        />
    </electromagnet_field>

    <constant_magnetic_field
        name="background_field"
        field="[fieldX] [fieldY] [fieldZ]"
            />

    <magnetic_superposition_field
        name="total_magnetic_field"
        use_caching="true"
            >
        <add_field name="background_field" enhancement="1.0" />
        <add_field name="trap_field" enhancement="1.0" />
    </magnetic_superposition_field>

</kemfield>