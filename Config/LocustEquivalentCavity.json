{
    "generators":
    [          
       "cavity-signal",
       "lpf-fft",
       "decimate-signal",
       "digitizer"
    ],

    "cavity-signal":
    {
        "transmitter": "kass-current",
	"e-gun": false,
        "voltage-check": "true",
        "lo-frequency": 0.98e9,               
	"equivalentR": 1.00,
	"equivalentL": 0.159e-6,
	"equivalentC": 0.159e-12,
        "tf-receiver-bin-width": 0.0005e9,
        "xml-filename": "${CMAKE_INSTALL_PREFIX}/config/LocustKass_Cavity_Template.xml"        
    },

    "simulation":
    {
        "egg-filename": "/tmp/locust_mc/build/output/locust_mc_100LR.egg",
        "n-records": 1,
        "record-size": 81920,
        "acquisition-rate": 210,
        "n-channels": 1
    },
  
    "gaussian-noise":
    {
    	"noise-floor-psd": 2.76e-22,
    	"domain": "time"
    },

    "digitizer":
    {
    	"v-range":  40.0,
    	"v-offset": -20.0
    }

}

