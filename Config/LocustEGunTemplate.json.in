{
    "generators":
    [          
       "cavity-signal",
       "lpf-fft",
       "decimate-signal",
       "digitizer"
    ],

    "cavity-signal":
    {
        "transmitter": "kass-current",
        "e-gun": true,
        "central-frequency": 25.9e9,
        "voltage-check": "true",
        "lo-frequency": 25.8741e9,               
        "tf-receiver-filename": "${CMAKE_INSTALL_PREFIX}/data/WR42_centerline_TF.txt",
        "tf-receiver-bin-width": 0.01e9,
        "xml-filename": "${CMAKE_INSTALL_PREFIX}/config/LocustKass_EGun_Template.xml"        
    },

    "simulation":
    {
        "egg-filename": "${CMAKE_INSTALL_PREFIX}/output/locust_mc.egg",
        "n-records": 1,
        "record-size": 81920,
        "acquisition-rate": 210,
        "n-channels": 1
    },
  
    "gaussian-noise":
    {
    	"noise-floor-psd": 2.76e-22,
    	"domain": "time"
    },

    "digitizer":
    {
    	"v-range":  5.0e-05,
    	"v-offset": -2.5e-05
    }

}

