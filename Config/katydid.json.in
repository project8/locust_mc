{
    "processor-toolbox":
    {
        "processors":
        [
            { "type": "egg-processor",          "name": "egg1" },
            { "type": "forward-fftw",           "name": "fft1" },
            { "type": "convert-to-power",       "name": "to-ps1" },
            { "type": "root-spectrogram-writer", "name":"waterfall-writer" },
            { "type": "basic-root-writer",   "name": "writer" }



        ],
        
        "connections":
        [
            {
                "signal": "egg1:header",
                "slot": "fft1:header",
                "order": 0
            },

            {
                "signal": "egg1:ts",
                "slot": "fft1:ts-fftw"
            },

            {
                "signal": "fft1:fft",
                "slot": "to-ps1:fs-fftw-to-psd"
            },
            
            {
				"signal": "egg1:ts",            
				"slot":  "writer:ts-fftw"
            },
            
            {
                "signal": "to-ps1:psd",
                "slot": "writer:psd"
            },

            {
                "signal": "fft1:fft",
                "slot": "writer:fs-fftw-phase"
            },
                                
            {
                "signal": "to-ps1:psd",
                "slot": "waterfall-writer:psd"
            },
            
            {
                "signal": "egg1:egg-done",
                "slot": "waterfall-writer:write-file"
            }

           


             
         
        ],
        
        "run-queue":
        [
            "egg1"
        ]
    },
        
    "egg1":
    {
        "filename": "${CMAKE_INSTALL_PREFIX}/bin/locust_mc.egg",
        "egg-reader": "egg3",
        "number-of-slices": 3,
        "slice-size": 8192,
        "stride": 8192,
        "dac":
        {
            "time-series-type": "fftw"
        }

    },



    "fft1":
    {
        "transform-flag": "ESTIMATE"
    },


    "waterfall-writer":
    {
        "output-file": "${CMAKE_INSTALL_PREFIX}/bin/katydidwaterfall.root",
        "file-flag": "recreate",
        "min-time": 0.0,
        "max-time": 0.025,
        "min-freq": 0.0e6,
        "max-freq": 200.0e6
    },



    "writer":
    {
        "output-file": "${CMAKE_INSTALL_PREFIX}/bin/basic.root",
        "file-flag": "recreate"
    }


}

